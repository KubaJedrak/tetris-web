    // temp
    const gridCellsTest = Array.from(document.querySelectorAll(".grid-cell"))
    const gridTest = Array.from(document.querySelector(".grid"))

    function removeCells() { 
      gridTest.forEach(index => {
        gridCellsTest.classList.remove("taken") 
      })
    }


    function clearGrid() {

      gridCell.forEach(index => {
        Element.classList.remove("tetromino");
        Element.classList.remove("taken")
      })
    }  

